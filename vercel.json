{
    "version": 2,
    "builds": [
        {
            "src": "object_detector.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/detect",
            "dest": "/object_detector.py"
        },
        {
            "src": "/(.*)",
            "dest": "/object_detector.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "handle": "error"
        }
    ]
}
