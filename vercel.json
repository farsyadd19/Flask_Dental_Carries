{
    "version": 2,
    "builds": [
        {
            "src": "object_detector.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "install": {
                    "includeFiles": {
                        "/vercel/path0/requirements.txt": "./requirements.txt"
                    },
                    "cmd": "pip3 install --disable-pip-version-check --target . --upgrade -r ./requirements.txt"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/detect",
            "dest": "/object_detector.py"
        },
        {
            "src": "/(.*)",
            "dest": "/object_detector.py"
        }
    ]
}
